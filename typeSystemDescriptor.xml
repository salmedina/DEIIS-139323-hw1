<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>typeSystemDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <types>
    <typeDescription>
      <name>Parser.Question</name>
      <description>Contains the question from input file</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>Text</name>
          <description>Original text from input file</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Parser.Questions</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>Text</name>
          <description>Contains the list of questions found in the input file</description>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Grader.Questions</name>
      <description>This type extends the Parser.Questions by adding scores to the questions</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>Score</name>
          <description>Contains the graded answers to the original question</description>
          <rangeTypeName>uima.cas.NonEmptyFloatList</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
